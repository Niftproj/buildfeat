{
    "featConfigure": {
        "featDocType": "featGenOptions",
        "featVersion": "2022.5.5",
        "featSupportFamily": "2022.x"
    },
    "projectType": "custom",
    "projectRoot": "./",
    "buildSystem": "make",
    "languages": ["c", "cxx", "python"],
    "compilerOptions": {
        "useSystem": true,
        "path": "/usr/bin/custom_root/",
        "c": "$compilerPath/bin/gcc",
        "c++": "$compilerPath/bin/gxx",
        "python": "$compilerPath/py/python"
    },
    "buildDir": "build",
    "binDir": "bin",
    "srcDir": "src",
    "sourceDepth": 1,
    "customs": {
        "MYPROJ_VERSION": "0.1b",
        "LINKER_SCRIPT": "$srcPath/script.x"
    },
    "copyMe": "\nall:\n\t@echo $(FEAT_OBJECTS)",
    "routines": {
        "build": "0"
    }
}